{
  "name": "astro",
  "type": "module",
  "version": "26.1.9",
  "lint-staged": {
    "*.{js,mjs,cjs,ts,mts,cts}": [
      "eslint",
      "prettier --write --ignore-unknown"
    ],
    "*.css": [
      "stylelint",
      "prettier --write --ignore-unknown"
    ],
    "!(*.{js,mjs,cjs,ts,mts,cts,css})": "prettier --write --ignore-unknown"
  },
  "scripts": {
    "dev": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro build && wrangler dev",
    "astro": "astro",
    "cf-typegen": "wrangler types",
    "prepare": "husky",
    "hash:dry": "tsx scripts/hash-password.ts --dry",
    "hash:upload": "tsx scripts/hash-password.ts",
    "hash:force": "tsx scripts/hash-password.ts --force"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/cloudflare": "^12.6.12",
    "@tailwindcss/vite": "^4.1.18",
    "astro": "^5.16.9",
    "jose": "^6.1.3",
    "markdown-it": "^14.1.0",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^25.0.8",
    "@types/yargs": "^17.0.35",
    "argon2": "^0.44.0",
    "cloudflare": "^5.2.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "glob": "^13.0.0",
    "globals": "^17.0.0",
    "gray-matter": "^4.0.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "slugify": "^1.6.6",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^39.0.1",
    "tsx": "^4.21.0",
    "typescript-eslint": "^8.53.0",
    "wrangler": "^4.59.1",
    "yargs": "^18.0.0"
  }
}
