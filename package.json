{
  "name": "astro",
  "type": "module",
  "version": "26.1.9",
  "lint-staged": {
    "*.{js,mjs,cjs,ts,mts,cts}": [
      "eslint",
      "prettier --write --ignore-unknown"
    ],
    "*.css": [
      "stylelint",
      "prettier --write --ignore-unknown"
    ],
    "!(*.{js,mjs,cjs,ts,mts,cts,css})": "prettier --write --ignore-unknown"
